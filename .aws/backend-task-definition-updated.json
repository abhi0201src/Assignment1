{
    "family": "django-nextjs-backend",
    "networkMode": "awsvpc",
    "executionRoleArn": "arn:aws:iam::629385237449:role/django-nextjs-ecs-execution-role",
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "256",
    "memory": "512",
    "containerDefinitions": [
        {
            "name": "backend",
            "image": "629385237449.dkr.ecr.us-west-2.amazonaws.com/django-nextjs-backend:latest",
            "portMappings": [
                {
                    "containerPort": 8000,
                    "protocol": "tcp"
                }
            ],
            "environment": [
                {
                    "name": "DJANGO_SETTINGS_MODULE",
                    "value": "RestaurantCore.settings"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/django-nextjs-backend",
                    "awslogs-region": "us-west-2",
                    "awslogs-stream-prefix": "backend"
                }
            }
        }
    ]
}
